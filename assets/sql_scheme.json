{
    //** column names with lower case
    //** data ONLY upper case, with exception for 'info'
    "GEO":
    //**https://wbdata.readthedocs.io/en/stable/
    {
        "iso2": "TEXT PRIMARY KEY",
        "country": "TEXT",
        "iso2_region": "TEXT",
        "region": "TEXT",
        "currency": "TEXT",
        "currency_code": "TEXT",
        "currency_digit": "INTEGER",
        "last_upd": "TIMESTAMP"
    },

    "INDEXES_DESC": { //**INDEX is reserved keyword
        "HASH": "TEXT PRIMARY KEY", //**from symbol, name, 'INDEXES'
        "components": "TEXT",
        "country": "TEXT",
        "industry": "TEXT",
        "info": "TEXT",
        "from_date": "TIMESTAMP",
        "to_date": "TIMESTAMP",
        "FOREIGN": { "country": "GEO(iso2)" }
    },

    "INDEXES": {
        "HASH": "TEXT",
        "symbol": "TEXT",
        "name": "TEXT",
        "vol": "INTEGER",
        "val": "INTEGER",
        "date": "TIMESTAMP",
        "FOREIGN": { "HASH": "INDEXES_DESC(HASH)" }
    },
    "ETF_DESC": {
        "HASH": "TEXT PRIMARY KEY", //**from symbol, name, 'ETF'
        "components": "TEXT",
        "country": "TEXT",
        "industry": "TEXT",
        "info": "TEXT",
        "from_date": "TIMESTAMP",
        "to_date": "TIMESTAMP",
        "FOREIGN": { "country": "GEO(iso2)" }
    },
    "ETF": {
        "HASH": "TEXT",
        "symbol": "TEXT",
        "name": "TEXT",
        "vol": "INTEGER",
        "val": "INTEGER",
        "date": "TIMESTAMP",
        "FOREIGN": { "HASH": "ETF_DESC(HASH)" }
    },
    "COMODITIES_DESC": {
        "HASH": "TEXT PRIMARY KEY", //**from symbol, name, 'ETF'
        "sector": "TEXT",
        "info": "TEXT",
        "from_date": "TIMESTAMP",
        "to_date": "TIMESTAMP"
    },
    "COMODITIES": {
        "HASH": "TEXT",
        "symbol": "TEXT",
        "vol": "INTEGER",
        "val": "INTEGER",
        "date": "TIMESTAMP",
        "FOREIGN": { "HASH": "COMODITIES_DESC(HASH)" }
    },

    "STOCK": {
        "hash": "TEXT",
        "symbol": "TEXT",
        "vol": "INTEGER",
        "val": "INTEGER",
        "date": "TIMESTAMP",
        "FOREIGN": { "HASH": "STOCK_DESC(HASH)" }
    },

    "STOCK_DESC": {
        "hash": "TEXT PRIMARY KEY", //**from symbol, name, 'STOCK'
        "country": "TEXT",
        "industry": "TEXT",
        "info": "TEXT",
        "from_date": "TIMESTAMP",
        "to_date": "TIMESTAMP",
        "FOREIGN": { "country": "GEO(iso2)" }
    },
//** all prices in SQL are written in USD, so we need relation to USD only
    "CURRENCY": { 
        "iso2": "TEXT",
        "toUSD": "INTEGER",
        "date": "TIMESTAMP",
        "FOREIGN":{"iso2":"GEO(iso2)"}
    }
}